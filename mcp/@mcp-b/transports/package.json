{
  "name": "@mcp-b/transports",
  "version": "1.0.3",
  "description": "Transports for the in browser transport protocols",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "browser",
    "transport",
    "browser-extension",
    "tab-transport",
    "chrome-extension"
  ],
  "homepage": "https://github.com/MiguelsPizza/WebMCP#readme",
  "bugs": {
    "url": "https://github.com/MiguelsPizza/WebMCP/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelsPizza/WebMCP.git",
    "directory": "packages/transports"
  },
  "license": "MIT",
  "author": "Alex Nahas",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.326",
    "@types/node": "^22.15.21",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.15.0"
  },
  "peerDependenciesMeta": {
    "@modelcontextprotocol/sdk": {
      "optional": false
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "check": "biome check --write .",
    "clean": "rm -rf dist .turbo",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "publish:dry": "pnpm publish --access public --dry-run",
    "publish:npm": "pnpm publish --access public",
    "typecheck": "tsc --noEmit",
    "version:major": "pnpm version major --no-git-tag-version",
    "version:minor": "pnpm version minor --no-git-tag-version",
    "version:patch": "pnpm version patch --no-git-tag-version"
  }
}