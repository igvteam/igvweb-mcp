{
  "manifest_version": "0.1",
  "name": "igvweb-mcp",
  "version": "0.0.1",
  "description": "Control igv-web via Model Context Protocol",
  "author": {
    "name": "Jim Robinson"
  },
  "server": {
    "type": "node",
    "entry_point": "dist/igvweb-mcp.js",
    "mcp_config": {
      "command": "node",
      "args": [
        "${__dirname}/dist/igvweb-mcp.js",
        "--host",
        "127.0.0.1",
        "port",
        "60141"
      ]
    }
  }
}